<view class="container">
  <view>
    <swiper class="swiper_box" indicator-dots="{{indicatorDots}}" vertical="{{vertical}}" autoplay='{{autoplay}}' interval='{{interval}}'>
      <block wx:for="{{goodsPicsInfo}}">
        <swiper-item>
          <image src="{{item.picurl}}" class="slide-image"/>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <view class="shopping_container">
    <view class="title">{{shoppingDetails.title}}</view>
    <view class="reason">{{shoppingDetails.reason}}</view>
  </view>

  <view>
    <view></view>
  </view>
</view>
